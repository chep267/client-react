{
    "name": "chep-react",
    "sideEffects": false,
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "homepage": "https://chep267.github.io",
    "scripts": {
        "dev": "vite",
        "dev:debug": "DEBUG=\"vite:transform\" vite",
        "build:staging": "tsc && vite build --mode staging",
        "build:dev": "tsc && vite build --mode development",
        "build": "tsc && vite build && cp dist/index.html dist/404.html",
        "preview": "vite preview",
        "lint": "eslint . --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0",
        "lint:fix": "eslint --fix --ext js,jsx,ts,tsx src/",
        "prettier": "prettier --check \"src/**/(*.tsx|*.ts|*.css|*.scss)\"",
        "prettier:fix": "prettier --write \"src/**/(*.tsx|*.ts|*.css|*.scss)\"",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d dist",
        "update": "ncu -u && npm i --legacy-peer-deps",
        "analyze": "source-map-explorer 'dist/assets/*.js'",
        "test:tree-sharking": "npm run build && node test.tree-sharking.mjs"
    },
    "dependencies": {
        "@emoji-mart/data": "^1.1.2",
        "@emoji-mart/react": "^1.1.1",
        "@emotion/react": "^11.11.4",
        "@emotion/styled": "^11.11.0",
        "@hookform/resolvers": "^3.3.4",
        "@mui/base": "5.0.0-beta.29",
        "@mui/icons-material": "^5.15.11",
        "@mui/lab": "5.0.0-alpha.158",
        "@mui/material": "^5.15.11",
        "@mui/styles": "^5.15.11",
        "@mui/utils": "^5.15.11",
        "@mui/x-date-pickers": "^6.19.5",
        "@tanstack/react-query": "^5.24.1",
        "@tsparticles/engine": "^3.3.0",
        "@tsparticles/react": "^3.0.0",
        "@tsparticles/slim": "^3.3.0",
        "axios": "^1.6.7",
        "classnames": "^2.5.1",
        "crypto-es": "^2.1.0",
        "dayjs": "^1.11.10",
        "emoji-mart": "^5.5.2",
        "firebase": "^10.8.1",
        "js-cookie": "^3.0.5",
        "mui-tel-input": "^5.1.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.50.1",
        "react-intl": "^6.6.2",
        "react-router-dom": "^6.22.2",
        "tailwindcss": "^3.4.1",
        "yup": "^1.3.3"
    },
    "devDependencies": {
        "@tanstack/eslint-plugin-query": "^5.20.1",
        "@types/js-cookie": "^3.0.6",
        "@types/node": "^20.11.24",
        "@types/react": "^18.2.61",
        "@types/react-dom": "^18.2.19",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "@vitejs/plugin-basic-ssl": "^1.1.0",
        "@vitejs/plugin-react": "^4.2.1",
        "autoprefixer": "^10.4.17",
        "babel-plugin-direct-import": "^1.0.0",
        "babel-plugin-transform-imports": "^2.0.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-formatjs": "^4.12.2",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.5",
        "gh-pages": "^6.1.1",
        "postcss": "^8.4.35",
        "prettier": "^3.2.5",
        "rollup-plugin-visualizer": "^5.12.0",
        "source-map-explorer": "^2.5.3",
        "typescript": "^5.3.3",
        "vite": "^5.1.4"
    }
}
